{
  "name": "stackable-crawler",
  "version": "0.0.3",
  "description": "middleware based lightweight crawler framework",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src -d lib",
    "watch": "babel src --watch -d lib",
    "prepublish": "npm test && npm run build",
    "lint": "eslint {src,test} --fix",
    "unittest": "mocha --compilers js:babel-register --recursive test",
    "test": "npm run lint && npm run unittest"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airtoxin/stackable-crawler.git"
  },
  "keywords": [
    "crawler",
    "spider",
    "scraper",
    "promise",
    "async",
    "queue",
    "framework",
    "middleware",
    "stack"
  ],
  "author": "airtoxin <airtoxin@icloud.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/airtoxin/stackable-crawler/issues"
  },
  "homepage": "https://github.com/airtoxin/stackable-crawler#readme",
  "dependencies": {
    "babel-polyfill": "6.13.0",
    "cheerio": "0.20.0",
    "eventemitter3": "1.2.0",
    "jquery": "3.1.0",
    "nightmare": "2.6.1",
    "nightmare-evaluate-async": "0.1.4",
    "request": "2.74.0",
    "valid-url": "1.0.9"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-eslint": "6.1.2",
    "babel-plugin-transform-builtin-extend": "1.1.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-es2016": "6.11.3",
    "babel-preset-stage-2": "6.13.0",
    "babel-preset-stage-3": "6.11.0",
    "babel-register": "6.11.6",
    "eslint": "3.3.1",
    "eslint-config-airbnb": "10.0.1",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-import": "1.13.0",
    "eslint-plugin-jsx-a11y": "2.1.0",
    "eslint-plugin-react": "6.1.1",
    "from": "0.1.3",
    "is": "3.1.0",
    "mocha": "3.0.2"
  }
}
